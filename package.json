{
  "name": "fol-parser",
  "version": "1.0.0",
  "description": "A parser for first-order logic formulas in LaTeX",
  "license": "MIT",
  "author": "Joseph Jackson",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
